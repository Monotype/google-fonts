    <script type='text/javascript'>
      function toggle(toggle_class){
        var elements = document.getElementsByTagName('tr')
        for (var i = 0; i < elements.length; i++){
          var e = elements[i]
          if (e.className == toggle_class) {
            if (e.style.display != 'none') {
              e.style.display = 'none'
            } else {
              e.style.display = 'table-row'
            }
          }
        }
      }
    </script>
    
    <script type='text/javascript'>
      function toggle_detail(my_id){
        var element = document.getElementById(my_id)
        var buttoncell = document.getElementById(my_id+"_cell")
        var button = document.getElementById(my_id+"_btn")
        if (element.style.display != 'none') {
          element.style.display = 'none'
          buttoncell.style.borderRadius = '0.0em'
          button.firstChild.data = '\u25bc'
        } else {
          element.style.display = ''
          buttoncell.style.borderRadius = '0.0em 0.0em 0 0'
          button.firstChild.data = '\u25b2'
        }
      }
    </script>
    
    <script type='text/javascript'>
      function toggle_all_results(){
        var elements = document.getElementsByClassName('result_body')
        for (var i = 0; i < elements.length; i++){
          var e = elements[i]
          if (e.style.display != 'none') {
            e.style.display = 'none'
          } else {
            e.style.display = ''
          }
        }
      }
    </script>
